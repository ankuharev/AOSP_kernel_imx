&flexcan1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_flexcan1_1>;
	trx-en-gpio = <&gpio5 6 1>;
	status = "okay";
};

&flexcan2 {
	pinctrl-names = "default";
